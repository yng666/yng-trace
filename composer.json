{
    "name": "yng/yng-trace",
    "description": "yngphp debug trace",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "YNG",
            "email": "xxxxxx@qq.com"
        }
    ],
    "require": {
    	"php": ">=7.1.0",
        "yng/yng-framework": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Yng\\Trace\\": "src/"
        }
    },
    "extra": {
        "yng":{
            "services":[
                "Yng\\Trace\\Service"
            ],
            "config":{
                "trace": "src/config.php"
            }
        }
    },    
    "minimum-stability": "dev",
    "prefer-stable": true
}
